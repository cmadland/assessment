@report{stakerRiseK12Blended2011,
  title = {The {{Rise}} of {{K}}-12 {{Blended Learning}}: {{Profiles}} of {{Emerging Models}}},
  author = {Staker, Heather and {Innosight Institute}},
  date = {2011-05-01},
