@article{vuralStudentIncivilityHigher2020,
  title = {Student {{Incivility}} in {{Higher Education}}},
  author = {Vural, Levent and Bacioglu, Seda Donat},
  date = {2020},
  journaltitle = {International Journal of Progressive Education},
  volume = {16},
