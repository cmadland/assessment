  pagetotal = {159},
  keywords = {open educational resources; learning design; collaborative design; open source software; OERu},
  file = {/Users/cmadland/Zotero/storage/8ZILRRH5/Devries 2013.docx;/Users/cmadland/Zotero/storage/XHYVZWYY/etd8126_IDeVries.pdf}
